# Brewfile
# Complete development system (combines all categories)
# Run: brew bundle --file=packages/brew/Brewfile

# Include all package categories
eval "$(<$(dirname "${BASH_SOURCE[0]:-$0}")/Brewfile.base)"
eval "$(<$(dirname "${BASH_SOURCE[0]:-$0}")/Brewfile.cli)"
eval "$(<$(dirname "${BASH_SOURCE[0]:-$0}")/Brewfile.development)"
eval "$(<$(dirname "${BASH_SOURCE[0]:-$0}")/Brewfile.gui)"
